{
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES2019"],
    "module": "commonjs",
    "baseUrl": ".",
    /* Base directory to resolve non-absolute module names. */
    "paths": {
      "app-util/*": ["./src/util/*"],
      "app-store": ["./src/store"],
      "app-config": ["./src/config.js"],
      "app-packages/*": ["./src/packages/*"],
      "app-helper/*": ["./src/helper/*"],
      "app-helper": ["./src/helper"],
      "@data": ["./src/data"],
      "@shared": ["./src/shared"]
    },
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false
  },
  "exclude": ["node_modules"]
}
